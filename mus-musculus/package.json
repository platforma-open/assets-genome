    "block-software": {
      "entrypoints": {
        "main": {
          "asset": {
            "type": "asset",
            "registry": "platforma-open",
            "root": "./indexed_genome/Mus_musculus"
          }
        }
      }
    },
    "files": [
      "dist/"
    ],
    "license": "UNLICENSED",
    "devDependencies": {
      "@platforma-sdk/package-builder": "catalog:"
    }
}