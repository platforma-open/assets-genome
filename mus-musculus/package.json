{
    "name": "@platforma-open/milaboratories.genome-assets.mus-musculus",
    "version": "1.0.0",
    "description": "Assets for Mus Musculus",
    "scripts": {
      "cleanup": "rm -rf ./pkg-*.tgz && rm -rf ./build/ && rm -rf ./dist/",
      "build": "echo 'Start build' && pl-pkg build",
      "prebuild": "echo 'Start pre-rebuild' && ../build-everything.sh ./genomeFileUrls.json",
      "prepublishOnly": "pl-pkg publish"
